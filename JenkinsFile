pipeline {
	agent any
	
	stages {
		stage ('Compilação') {
			steps {
				withMaven(maven: 'MAVEN_3_5_4') {
					bat 'mvn clean install'
				}
			}
		}
		
		stage ('Deploy') {
			steps {
				bat 'set > env.txt'
				for (String i : readFile('env.txt').split("\r?\n")) {
 			    	println i
				}
			}
		}
	}

}